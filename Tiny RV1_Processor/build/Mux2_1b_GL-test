    %vpi_call/w 3 71 "$write", "\033[31mFAILED\033[0m" {0 0 0};
T_0.7 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x1088390_0, 0, 1;
    %jmp T_0.5;
T_0.4 ;
    %load/vec4 v0x1088430_0;
