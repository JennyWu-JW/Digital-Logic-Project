TD_Top.check ;
    %load/vec4 v0x1088390_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %load/vec4 v0x102e430_0;
    %store/vec4 v0x1088e40_0, 0, 1;
    %load/vec4 v0x1057c90_0;
    %store/vec4 v0x1088ee0_0, 0, 1;
    %load/vec4 v0x1086850_0;
    %store/vec4 v0x10890b0_0, 0, 1;
    %delay 8, 0;
    %load/vec4 v0x1088430_0;
    %cmpi/ne 0, 0, 32;
    %jmp/0xz  T_0.2, 4;
    %vpi_call/w 3 61 "$display", "%3d: %b %b %b > %b", v0x10882b0_0, v0x1088e40_0, v0x1088ee0_0, v0x10890b0_0, v0x1088fe0_0 {0 0 0};
T_0.2 ;
    %load/vec4 v0x10867b0_0;
    %load/vec4 v0x10867b0_0;
    %load/vec4 v0x1088fe0_0;
    %xor;
    %load/vec4 v0x10867b0_0;
    %xor;
    %cmp/ne;
    %jmp/0xz  T_0.4, 6;
    %load/vec4 v0x1088430_0;
    %cmpi/ne 0, 0, 32;
    %jmp/0xz  T_0.6, 4;
    %vpi_call/w 3 68 "$display", "\012\033[31mERROR\033[0m (cycle=%0d): %s != %s (%b != %b)", v0x10882b0_0, "dut_out", "out", v0x1088fe0_0, v0x10867b0_0 {0 0 0};
    %jmp T_0.7;
