TD_Top.check ;
    %load/vec4 v0xb2faf0_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %load/vec4 v0xaca430_0;
    %store/vec4 v0xb30610_0, 0, 1;
    %delay 1, 0;
    %load/vec4 v0xb2cf10_0;
    %store/vec4 v0xb307f0_0, 0, 1;
    %load/vec4 v0xaf9a50_0;
    %store/vec4 v0xb306b0_0, 0, 1;
    %delay 7, 0;
    %load/vec4 v0xb2fb90_0;
    %cmpi/ne 0, 0, 32;
    %jmp/0xz  T_0.2, 4;
    %vpi_call/w 3 64 "$display", "%3d: %b %b %b > %b", v0xb2fa10_0, v0xb30610_0, v0xb307f0_0, v0xb306b0_0, v0xb30750_0 {0 0 0};
T_0.2 ;
    %load/vec4 v0xaf7890_0;
    %load/vec4 v0xaf7890_0;
    %load/vec4 v0xb30750_0;
    %xor;
    %load/vec4 v0xaf7890_0;
    %xor;
    %cmp/ne;
    %jmp/0xz  T_0.4, 6;
    %load/vec4 v0xb2fb90_0;
    %cmpi/ne 0, 0, 32;
    %jmp/0xz  T_0.6, 4;
    %vpi_call/w 3 71 "$display", "\012\033[31mERROR\033[0m (cycle=%0d): %s != %s (%b != %b)", v0xb2fa10_0, "dut_q", "q", v0xb30750_0, v0xaf7890_0 {0 0 0};
    %jmp T_0.7;
T_0.6 ;
    %vpi_call/w 3 74 "$write", "\033[31mFAILED\033[0m" {0 0 0};
T_0.7 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0xb2faf0_0, 0, 1;
    %jmp T_0.5;
T_0.4 ;
    %load/vec4 v0xb2fb90_0;
    %cmpi/e 0, 0, 32;
    %jmp/0xz  T_0.8, 4;
    %vpi_call/w 3 79 "$write", "\033[32m", ".", "\033[0m" {0 0 0};
T_0.8 ;
T_0.5 ;
    %delay 2, 0;
T_0.0 ;
    %end;
