TD_Top.check ;
    %load/vec4 v0x1cc7c10_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %load/vec4 v0x1c73430_0;
    %store/vec4 v0x1cc8580_0, 0, 1;
    %delay 1, 0;
    %load/vec4 v0x1cc69c0_0;
    %store/vec4 v0x1cc8620_0, 0, 1;
    %delay 8, 0;
    %load/vec4 v0x1cc7ce0_0;
    %cmpi/ne 0, 0, 32;
    %jmp/0xz  T_0.2, 4;
    %vpi_call/w 3 60 "$display", "%3d: %b %b > %b", v0x1cc7b30_0, v0x1cc8580_0, v0x1cc8620_0, v0x1cc8720_0 {0 0 0};
T_0.2 ;
    %load/vec4 v0x1cc6a80_0;
    %load/vec4 v0x1cc6a80_0;
    %load/vec4 v0x1cc8720_0;
    %xor;
    %load/vec4 v0x1cc6a80_0;
    %xor;
    %cmp/ne;
    %jmp/0xz  T_0.4, 6;
    %load/vec4 v0x1cc7ce0_0;
    %cmpi/ne 0, 0, 32;
    %jmp/0xz  T_0.6, 4;
    %vpi_call/w 3 67 "$display", "\012\033[31mERROR\033[0m (cycle=%0d): %s != %s (%b != %b)", v0x1cc7b30_0, "dut_q", "q", v0x1cc8720_0, v0x1cc6a80_0 {0 0 0};
    %jmp T_0.7;
