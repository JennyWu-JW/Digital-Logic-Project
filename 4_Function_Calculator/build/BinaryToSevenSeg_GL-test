    %vpi_call/w 3 63 "$write", "\033[31mFAILED\033[0m" {0 0 0};
T_0.7 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x231f5a0_0, 0, 1;
    %jmp T_0.5;
T_0.4 ;
    %load/vec4 v0x231f640_0;
    %cmpi/e 0, 0, 32;
    %jmp/0xz  T_0.8, 4;
