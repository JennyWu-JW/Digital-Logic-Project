    %load/vec4 v0x231f5a0_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %load/vec4 v0x22ac430_0;
    %store/vec4 v0x231fee0_0, 0, 4;
    %delay 8, 0;
    %load/vec4 v0x231f640_0;
    %cmpi/ne 0, 0, 32;
    %jmp/0xz  T_0.2, 4;
    %vpi_call/w 3 53 "$display", "%3d: %b (%d) > %b", v0x231f4c0_0, v0x231fee0_0, v0x231fee0_0, v0x231ff80_0 {0 0 0};
T_0.2 ;
    %load/vec4 v0x2319560_0;
    %load/vec4 v0x2319560_0;
    %load/vec4 v0x231ff80_0;
    %xor;
    %load/vec4 v0x2319560_0;
    %xor;
    %cmp/ne;
    %jmp/0xz  T_0.4, 6;
    %load/vec4 v0x231f640_0;
    %cmpi/ne 0, 0, 32;
    %jmp/0xz  T_0.6, 4;
