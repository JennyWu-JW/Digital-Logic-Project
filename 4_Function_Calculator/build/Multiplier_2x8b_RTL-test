TD_Top.check ;
    %load/vec4 v0x1511c90_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %load/vec4 v0x14b8430_0;
    %store/vec4 v0x1512ab0_0, 0, 8;
    %load/vec4 v0x1510830_0;
    %store/vec4 v0x1512b50_0, 0, 2;
    %delay 8, 0;
    %load/vec4 v0x1511d30_0;
    %cmpi/ne 0, 0, 32;
    %jmp/0xz  T_0.2, 4;
    %vpi_call/w 3 57 "$display", "%3d: %b * %b (%3d * %d) > %b (%3d)", v0x1511bb0_0, v0x1512ab0_0, v0x1512b50_0, v0x1512ab0_0, v0x1512b50_0, v0x1512c50_0, v0x1512c50_0 {0 0 0};
T_0.2 ;
    %load/vec4 v0x1510910_0;
    %load/vec4 v0x1510910_0;
    %load/vec4 v0x1512c50_0;
    %xor;
    %load/vec4 v0x1510910_0;
    %xor;
    %cmp/ne;
    %jmp/0xz  T_0.4, 6;
    %load/vec4 v0x1511d30_0;
    %cmpi/ne 0, 0, 32;
    %jmp/0xz  T_0.6, 4;
    %vpi_call/w 3 65 "$display", "\012\033[31mERROR\033[0m (cycle=%0d): %s != %s (%b != %b)", v0x1511bb0_0, "dut_prod", "prod", v0x1512c50_0, v0x1510910_0 {0 0 0};
    %jmp T_0.7;
T_0.6 ;
    %vpi_call/w 3 68 "$write", "\033[31mFAILED\033[0m" {0 0 0};
T_0.7 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x1511c90_0, 0, 1;
    %jmp T_0.5;
T_0.4 ;
    %load/vec4 v0x1511d30_0;
    %cmpi/e 0, 0, 32;
