TD_Top.check ;
    %load/vec4 v0x2630090_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %load/vec4 v0x25d5430_0;
    %store/vec4 v0x2630a00_0, 0, 1;
    %delay 1, 0;
    %load/vec4 v0x2602850_0;
    %store/vec4 v0x2630af0_0, 0, 1;
    %delay 7, 0;
    %load/vec4 v0x2630160_0;
    %cmpi/ne 0, 0, 32;
    %jmp/0xz  T_0.2, 4;
    %vpi_call/w 3 60 "$display", "%3d: %b %b > %b", v0x262ffb0_0, v0x2630a00_0, v0x2630af0_0, v0x2630be0_0 {0 0 0};
T_0.2 ;
    %load/vec4 v0x25fcc90_0;
    %load/vec4 v0x25fcc90_0;
    %load/vec4 v0x2630be0_0;
    %xor;
    %load/vec4 v0x25fcc90_0;
    %xor;
    %cmp/ne;
    %jmp/0xz  T_0.4, 6;
    %load/vec4 v0x2630160_0;
    %cmpi/ne 0, 0, 32;
    %jmp/0xz  T_0.6, 4;
    %vpi_call/w 3 67 "$display", "\012\033[31mERROR\033[0m (cycle=%0d): %s != %s (%b != %b)", v0x262ffb0_0, "dut_q", "q", v0x2630be0_0, v0x25fcc90_0 {0 0 0};
    %jmp T_0.7;
T_0.6 ;
    %vpi_call/w 3 70 "$write", "\033[31mFAILED\033[0m" {0 0 0};
T_0.7 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x2630090_0, 0, 1;
    %jmp T_0.5;
T_0.4 ;
    %load/vec4 v0x2630160_0;
